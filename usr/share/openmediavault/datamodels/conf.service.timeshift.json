      "minimum": 0,
      "maximum": 999,
      "default": 0
    },
    "weekly": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999,
      "default": 0
    },
    "daily": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999,
      "default": 5
    },
    "hourly": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999,
      "default": 0
    },
    "boot": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999,
      "default": 0
    },
    "exclude": {
      "type": "string"
    }
  }
